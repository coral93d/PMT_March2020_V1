
@model  WebTimeSheetManagement.Models.MainAuditSheetEdit

@{
    ViewBag.Title = "Edit";
    var serializer = new System.Web.Script.Serialization.JavaScriptSerializer();
    string objJson = serializer.Serialize(Model);
    Layout = "~/Views/Shared/_LayoutSuperAdmin.cshtml";
}

<script>
    if ('@TempData["AuditCardMessage"]' != '') {
        alert('@TempData["AuditCardMessage"]');
    }

</script>


<h4 class="page-header">Modify AuditSheet</h4>
<div class="row">

    <div style="margin-top:10px;"></div>
    @using (Html.BeginForm("Edit", "AuditSheet"))
    {
        @Html.AntiForgeryToken()

        <div class="panel-body">
            <!--FORMS CODE-->
            <div class="col-md-3">
                @Html.Label("Select Form", new { @class = "manadatory" })
                <div class="col-md-10">
                    @Html.TextBoxFor(modelItem => Model.ListAuditMasterDetails[0].FormsCode, new { @class = "form-control", @maxlength = "50", @readonly = "readonly" })
                    @Html.HiddenFor(modelItem => Model.Audit_MasterID)

                </div>
            </div>

            @for (int i = 0; i < Model.ListAuditMaster.Count; i++)
            {
              <!--TRACK NAME-->
            <!--To SELECT TRACK NAME-->
                <div class="col-md-3">
                    @Html.Label("Call Type", new { @class = "manadatory" })
                    <div class="col-md-10">
                        @Html.DropDownListFor(model => model.ListAuditMaster[i].Call_p1,
                            new List<SelectListItem>()
                            {
                            new SelectListItem() { Text = "----Select----" },
                            new SelectListItem() { Text = "Calling", Value =  "Calling"},
                            new SelectListItem() { Text = "Non_Calling", Value = "Non_Calling"}
                            }, new { @class = "form-control" })
                        

                    </div>
                </div>

              <!--MODE OF MONITORING-->
            <div class="col-md-3">
                <div class="col-md-10">
                    @Html.Label("Mode_of_Monitoring", new { @class = "manadatory" })
                    @Html.TextBoxFor(modelItem => Model.ListAuditMaster[i].Mode_of_Monitoring, new { @class = "form-control", @maxlength = "50", @readonly = "readonly" })

                </div>
            </div>

              <!--USERNAME-->
            <div class="col-md-3">
                @Html.LabelFor(modelItem => Model.ListAuditMaster[i].UserName, new { @class = "manadatory" })
                <div class="col-md-10">
                    @Html.TextBoxFor(modelItem => Model.ListAuditMaster[i].UserName, new { @class = "form-control", @maxlength = "50", @readonly = "readonly" })
                </div>
            </div>


              <!--DUMMY-->
            <div class="col-md-3">
                <label id="label1">
                </label>
                <div id="_panelmainbuttonnnn" style="display:none;">

                    <div class="col-md-10">
                        <!--<input id="Submit" onclick="return ValidateData();" class="btn btn-success" type="submit" value="Save Details">-->
                        @Html.ActionLink("Cancel", "Add", "AuditSheet", null, new { @class = "btn btn-danger" })

                    </div>
                </div>
            </div>

            <div id="_panelmains" >

                <div class="panel-body">

                    <div style="margin:10px"></div>

                    <br />

                </div>

                <!--DISPLAY COMPLETE USER INFO-->
                <div id="myTable" class="login-container" style="width: 99%; position: relative;">
                    <div>
                        <div class="col-md-6 login-form-1">

                            <div class="form-group">
                                <label style="color:black">User Name</label>

                                @Html.TextBoxFor(modelItem => Model.ListAuditMaster[i].UserName, new { @class = "form-control", @maxlength = "50", @readonly = "readonly" })
                            </div>
                            <div class="form-group">
                                <label style="color:black">Process</label>
                                @Html.TextBoxFor(modelItem => Model.ListAuditMaster[i].Team_Name, new { @class = "form-control", @maxlength = "50", @readonly = "readonly" })
                            </div>

                        </div>
                        <div class="col-md-6 login-form-2">
                            <div class="login-logo">
                                <img src="https://image.ibb.co/n7oTvU/logo_white.png" alt="" />
                            </div>
                            <div class="form-group">
                                <label style="color:black">RM Name</label>
                                @Html.TextBoxFor(modelItem => Model.ListAuditMaster[i].Agent_Supervisor, new { @class = "form-control", @maxlength = "50", @readonly = "readonly" })
                            </div>
                            <div class="form-group">
                                <label style="color:black">Manager</label>
                                @Html.TextBoxFor(modelItem => Model.ListAuditMaster[i].Agent_Manager, new { @class = "form-control", @maxlength = "50", @readonly = "readonly" })
                            </div>
                        </div>
                    </div>
                </div>


                <br /><br /><br /><br />&nbsp;&nbsp;&nbsp;
            </div>
            <br /><br /><br /><br />

            }

