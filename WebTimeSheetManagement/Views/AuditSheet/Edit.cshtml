                            </div>

                            <!--DOS-->  
                            <div class="form-group">
                                <label class="col-md-4 control-label">DOS</label>
                                <div class="col-md-8 inputGroupContainer">
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="fa fa-calendar-check-o"></i></span>
                                        @Html.TextBoxFor(modelItem => Model.ListAuditMaster[i].Date_Of_Service, new { @class = "form-control", @maxlength = "50" })

                                    </div>
                                </div>
                            </div>

                            <!--ACIS ID-->
                            <div class="form-group">
                                <label class="col-md-4 control-label">ACIS ID</label>
                                <div class="col-md-8 inputGroupContainer">
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="glyphicon glyphicon-info-sign"></i></span>
                                        @Html.TextBoxFor(model => model.ListAuditMaster[i].ACIS_ID, new { @class = "form-control", @maxlength = "50" })
                                    </div>
                                </div>
                            </div>

                            <!--DNL REASON-->
                            <div class="form-group">
                                <label class="col-md-4 control-label">Denial Reason</label>
                                <div class="col-md-8 inputGroupContainer">
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="glyphicon glyphicon-cog"></i></span>
                                        @Html.TextBoxFor(model => model.ListAuditMaster[i].Denial_Reason, new { @class = "form-control", @maxlength = "50" })
                                    </div>
                                </div>
                            </div>
                            
                            <!--PAYOR ID-->
                            <div class="form-group">
                                <label class="col-md-4 control-label">Payor ID</label>
                                <div class="col-md-8 inputGroupContainer">
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="glyphicon glyphicon-info-sign"></i></span>
                                        @Html.TextBoxFor(model => model.ListAuditMaster[i].Payor_ID, new { @class = "form-control", @maxlength = "50" })
                                    </div>
                                </div>
                            </div>

                            <!--CURRENT LAC-->
                            <div class="form-group">
                                <label class="col-md-4 control-label">Current LAC</label>
                                <div class="col-md-8 inputGroupContainer">
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="fa fa-angle-right"></i></span>
                                        @Html.TextBoxFor(model => model.ListAuditMaster[i].Current_Lac, new { @class = "form-control", @maxlength = "50" })
                                    </div>
                                </div>
                            </div>

                            <!--PREVIOUS LAC-->
                            <div class="form-group">
                                <label class="col-md-4 control-label">Previous LAC</label>
                                <div class="col-md-8 inputGroupContainer">
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="fa fa-angle-left"></i></span>
                                        @Html.TextBoxFor(model => model.ListAuditMaster[i].Previous_Lac, new { @class = "form-control", @maxlength = "50" })
                                    </div>
                                </div>
                            </div>


                            <!--BUZZ RULE-->
                            <div class="form-group">
                                <label class="col-md-4 control-label">Buzz Rule Loaded or not?</label>
                                <div class="col-md-8 inputGroupContainer">
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="glyphicon glyphicon-phone"></i></span>
                                        @Html.DropDownListFor(model => model.ListAuditMaster[i].BuzzRule_Loaded_or_not,
                                                new List<SelectListItem>
                                                {new SelectListItem { Text = Model.ListAuditMaster[i].BuzzRule_Loaded_or_not,Value = Model.ListAuditMaster[i].BuzzRule_Loaded_or_not },
                                                new SelectListItem { Text = "Yes", Value = "Yes" },
                                                new SelectListItem { Text = "No", Value = "No" }

                                                }, new { @class = "form-control" })
                                    </div>
                                </div>
                            </div>

                         
                        </fieldset>

                    </div>
                </div>
            </div>
            <div class="col-md-6 col-lg-offset-5">

                <div id="_panelmainbutton">
                                        &nbsp;&nbsp;&nbsp;
                    <input id="Submit" class="btn btn-success" type="submit" value="Update">&nbsp;
                    @Html.ActionLink("Cancel", "Add", "AuditSheet", null, new { @class = "btn btn-danger" })
                </div>
            </div>
            <div style="margin-bottom:10px;"></div>
            <br /><br /><br />
        </div>

        }

    }
</div>






