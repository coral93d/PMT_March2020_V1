                                            @Html.HiddenFor(model => model.ListAuditMasterDetails[i].FieldID)
                                            @Html.HiddenFor(modelItem => Model.ListAuditMasterDetails[i].FormsCode, new { @class = "form-control", @maxlength = "50" })
                                           

                                            @Html.TextAreaFor(model => model.ListAuditMasterDetails[i].Questions, new { @class = "form-control", @rows = 2, @style = "border:none", @readonly = "readonly" })
                                        </td>
                                        <td>
                                            @Html.TextBoxFor(modelItem => Model.ListAuditMasterDetails[i].Metric, new { @class = "form-control", @maxlength = "50", @readonly = "readonly" })
                                        </td>

                                        <td>
                                            @Html.TextBoxFor(modelItem => Model.ListAuditMasterDetails[i].Max_Score, new { @class = "form-control", @maxlength = "50", @readonly = "readonly" })

                                        </td>
                                        <td>
